FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            hePsiThermo;
    mixture         pureMixture;
    transport       sutherland;      //Sutherland for air viscosity
    thermo          hConst;          
    equationOfState perfectGas;      //Perfect Gas Model
    specie          specie;
    energy          sensibleEnthalpy; //Total Enthalpy energy equation
}

mixture 
{
    specie
    {
        molWeight   28.96;
    }
    thermodynamics
    {
        Cp          1005;
        Hf          0;
    }
    transport
    {
        // Sutherland coefficients for Air
        As              1.458e-06;
        Ts              110.4;
    }
}

